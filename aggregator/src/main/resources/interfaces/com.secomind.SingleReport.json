{
  "interface_name": "com.secomind.SingleReport",
  "version_major": 0,
  "version_minor": 1,
  "type": "datastream",
  "ownership": "device",
  "aggregation": "object",
  "mappings": [
    {
      "endpoint": "/%{deviceId}/ErrorScans",
      "type": "integer",
      "database_retention_policy": "use_ttl",
      "database_retention_ttl": 31556952,
      "description": "Number of scans end in error in the last time frame"
    },
    {
      "endpoint": "/%{deviceId}/SuccessScans",
      "type": "integer",
      "database_retention_policy": "use_ttl",
      "database_retention_ttl": 31556952,
      "description": "Number of scans successfully done in the last time frame"
    }
  ]
}
